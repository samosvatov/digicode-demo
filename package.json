  "dependencies": {
    "bcrypt": "^3.0.6",
    "config": "^3.1.0",
    "cors": "^2.8.5",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "mongoose": "^5.6.0",
    "passport": "^0.4.0",
